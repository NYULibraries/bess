<?xml version="1.0" encoding="utf-8"?>
<configuration>

	<!-- basic application info, no trailing slash -->
	<!-- Base url to append before creating links to embed, so this applications base url -->
	<config name="base_url"></config>
	<!-- The Primo Deep Link Search Url, ends in dlSearch.do -->
	<config name="primo_dlsearch_url"><!--...dlSearch.do--></config>
	<!-- The Primo Search Url, ends in search.do -->
	<config name="primo_search_url"><!--...search.do--></config>
	<!-- Utilizes open source metalib searching PHP-library Xerxes -->
	<!-- https://code.google.com/p/xerxes-portal/ -->
	<config name="xerxes_search_url"></config>
	<!-- Utilizes open source SFX searching Rails application/link resolver Umlaut -->
	<!-- https://github.com/team-umlaut/umlaut -->
	<config name="umlaut_search_url"></config>

	<!-- application title -->
	<config name="app_title">BobCat</config>

	<!-- links and text for breadcrumbing, no trailing slashes -->
	<config name="app_url">http://bobcat.library.nyu.edu</config>
	<config name="home_url">https://library.nyu.edu</config>
	<config name="home_title">NYU Libraries</config>

	<!-- metalib connection info
		metalib_url: main metalib url with port if non-80, no trailing slash
		metalib_user: admin user name
		metalib_pass: admin password
		metalib_inst: metalib institute name
	-->
	<!-- Metalib X-Server settings, not implemented anywhere in application currently -->
	<config name="metalib_url" pass="false"></config>
	<config name="metalib_user" pass="false"></config>
	<config name="metalib_pass" pass="false"></config>
	<config name="metalib_inst" pass="false"></config>

	<config name="gauges_api" />

	<!-- views -->
	<views>

	<!-- Different institutions have different views of the searchbox, each is defined in a <view /> tag below -->

	<view default="true" id="NYU" title="NYU">
		<tabs>
			<tab system="primo" search="search" default="true" name="all" header="Search for Books &amp; More" description="Books &amp; e-books, journals &amp; e-journals, scripts, scores, archival materials, NYU dissertations, videos, sound recordings" title="Books &amp; More">
