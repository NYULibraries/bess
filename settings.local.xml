					<record_attributes name="addSearchFields[vl(212921975UI0)]" dlname="indexField">
						<record_attribute name="any">Anywhere in the record</record_attribute>
						<record_attribute name="title">In title</record_attribute>
						<record_attribute name="creator">In author/creator</record_attribute>
