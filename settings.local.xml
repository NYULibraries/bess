					<search_types name="addSearchFields[vl(1UIStartWith0)]" dlname="precisionOperator">
						<search_type name="contains">that contain my query words</search_type>
						<search_type name="exact">with my exact phrase</search_type>
						<search_type name="begins_with">starts with</search_type>
